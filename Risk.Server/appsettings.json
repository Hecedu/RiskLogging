{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path": "Logs/log.txt" }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore.SignalR": "Debug",
      "Microsoft.AspNetCore.Http.Connections": "Debug",
      "Microsoft.AspNetCore.SignalR.Internal": "Warning"
    }
  },
  "AllowedHosts": "*",
  "height": 12,
  "width": 12,
  "startingArmies": 50,
  "Syncfusion": "Mzk4NTQwQDMxMzgyZTM0MmUzMExCM3o4Um1Ubnltdks3MWdQYTBPZVNYRDlrdUNPS3k4MGdld0EweWZ1WDg9"
}
